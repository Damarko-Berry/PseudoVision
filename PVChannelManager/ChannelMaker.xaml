<Window x:Class="PVChannelManager.ChannelMaker"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PVChannelManager"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="ChannelMaker" Height="222" Width="809">
    <Grid>
        <TextBox x:Name="CN" HorizontalAlignment="Center" TextWrapping="Wrap" Text="" VerticalAlignment="Bottom" Width="613" Height="36" TextChanged="CN_TextChanged" Margin="0,0,0,133"/>
        <Button x:Name="Make" Content="Create" HorizontalAlignment="Center" Margin="0,0,0,37" Width="231" Height="20" VerticalAlignment="Bottom" Click="Make_Click"/>
        <ComboBox Name="TypeSelect" HorizontalAlignment="Left" Margin="178,0,0,0" VerticalAlignment="Center" Width="180" SelectionChanged="TypeSelect_SelectionChanged"/>
        <TextBlock  HorizontalAlignment="Left" Margin="98,0,0,0" TextWrapping="Wrap" Text="Channel Type" VerticalAlignment="Center"/>

    </Grid>
</Window>
